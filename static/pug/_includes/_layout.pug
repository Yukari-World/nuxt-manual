		block headSpace
	body
		header#top.md-show(role='banner')
			block headTitle
				include ./_header
			label(for='menuBtn') Menu
		input#menuBtn.panel-switch(type='checkbox')
		nav#menu.sidebar
			include ../_mixin/_sideList
		.content
			header.md-hide(role='banner')
				block headTitle
					include ./_header
			main
				block content
			footer(role='contentinfo')
				include ./_footer
		script(type='text/javascript', async='async', src='js/lazysizes.min.js')
		script(type='text/javascript', defer='defer', src='js/prism.min.js')
		//- script(type='text/javascript', defer='defer', src='js/index.js')
		script(type='module', defer='defer', src='js/Main.min.js')
		block jsContent
