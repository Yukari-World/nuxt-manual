doctype html
html(lang='ja')
	head
		meta(charset='utf-8')
		block title
			title HTML Technical Manual
		meta(name='description', content='HTMLで纏められた主にHTML技術関連のマニュアルページ')
		meta(name='viewport', content='width=device-width, initial-scale=1')
		meta(name='theme-color' content='#000011')
		meta(name='color-scheme' content='dark light')

		//- データの先読み
		link(rel='preload', as='style', type='text/css', href='https://fonts.googleapis.com/css?family=Noto+Sans+JP&display=swap')
		link(rel='preload', as='style', type='text/css', href='css/prismTomorrowNight.min.css')
		link(rel='preload', as='style', type='text/css', href='css/prism.css')
		link(rel='preload', as='style', type='text/css', href='css/style.min.css')
		link(rel='preload', as='script', type='application/javascript', href='js/lazysizes.min.js')
		link(rel='preload', as='script', type='application/javascript', href='js/vue.js')
		link(rel='preload', as='script', type='application/javascript', href='js/prism.min.js')
		link(rel='preload', as='script', type='application/javascript', href='https://cdnjs.cloudflare.com/ajax/libs/clipboard.js/2.0.0/clipboard.min.js')
		//- link(rel='preload', as='fetch', type='application/json', crossorigin='crossorigin', href='json/randomWord.json')
		link(rel='modulepreload', href='js/Main.min.js')
		link(rel='modulepreload', href='js/common.min.js')
		link(rel='modulepreload', href='js/ajax-response.min.js')
		link(rel='manifest', href='manifest.json')
		block preloader

		//- データの本読み込み
		link(rel='stylesheet', type='text/css', href='https://fonts.googleapis.com/css?family=Noto+Sans+JP&display=swap')
		link(rel='stylesheet', type='text/css', href='css/prismTomorrowNight.min.css')
		link(rel='stylesheet', type='text/css', href='css/prism.css', media='print, (prefers-color-scheme: light)')
		link(rel='stylesheet', type='text/css', href='css/style.min.css')
		script(type='text/javascript', defer='defer', src='js/vue.js')
		//- script(type='text/javascript').
		//- 	/*! loadCSS. [c]2017 Filament Group, Inc. MIT License */
