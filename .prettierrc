{
	"arrowParens": "always",
	"printWidth": 120,
	"semi": true,
	"singleQuote": true,
	"tabWidth": 4,
	"useTabs":true,
	"overrides": [
		{
			"files": "*.js",
			"options": {
				"parser": "babel",
				"trailingComma": "all"
			}
		}, {
			"files": "*.scss",
			"options": {
				"parser": "scss",
				"singleQuote": false
			}
		}, {
			"files": "*.vue",
			"options": {
				"parser": "vue"
			}
		}
	]
}
